AXIOM -> class

class ->  
	classdec. identifier. lcrch. classbody rcrch. semic. class |
	''

classbody ->
	visibility. type. identifier. semic. classbody | //comment testing
	''

classdec.("class")
visibility.("(public|private|protected)")
type.("int")
lcrch.("\{")
rcrch.("\}")
